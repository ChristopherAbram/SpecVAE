            "kingdom_id":               [],
            "superclass_id":            [6, 18, 14, 1, 10, 12, 0, 7, 5],
            "class_id":                 [198, 96, 32, 173, 61, 238, 94, 70, 106, 131, 65, 124, 43, 172, 190]
        }
    },
    "sessions": [
        {
            "--session": "01",
            "--use-cuda": true,
            "--gpu-device": 0,
            "--beta": 0.01
        },
        {
            "--session": "02",
            "--use-cuda": true,
            "--gpu-device": 0,
            "--beta": 0.1
        },
        {
            "--session": "03",
            "--use-cuda": true,
            "--gpu-device": 0,
            "--beta": 0.2
        },
        {
            "--session": "04",
            "--use-cuda": true,
            "--gpu-device": 0,
            "--beta": 0.5
        },
        {
            "--session": "05",
            "--use-cuda": true,
            "--gpu-device": 0,
            "--beta": 1.0
        }
    ]
}