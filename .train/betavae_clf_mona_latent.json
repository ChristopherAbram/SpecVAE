{
    "name": "betavae_clf_mona_latent",
    "environment": "specvae",
    "script": "script/train_vae_clf_single.py",
    "parameters": {
        "--model-name": "betavae_clf_latent",
        "--dataset": "MoNA",
        "--n-samples": -1,
        "--max-mz": 2500.0,
        "--n-peaks": [50],
        "--min-intensity": [0.001],
        "--rescale-intensity": [false],
        "--normalize-intensity": true,
        "--normalize-mass": true,
        "--n-epochs": 30,
        "--batch-size": 128,
        "--learning-rate": 0.001,
        "--layer-config": [
            "[[$indim, 1600, 400, 3],  [3,  400, 1600, $indim]]",
            "[[$indim, 1600, 400, 10], [10, 400, 1600, $indim]]"
        ],
        "--input-columns": "['spectrum']",
        "--target-column": {
            "--target-column":          "--class-subset",
            "ionization_mode_id":       [],
