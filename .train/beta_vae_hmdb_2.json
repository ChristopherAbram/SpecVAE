{
    "name": "betavae_hmdb_2",
    "environment": "specvae",
    "script": "specvae/train_vae_single.py",
    "parameters": {
        "--model-name": "beta_vae_2",
        "--dataset": "HMDB",
        "--n-samples": -1,
        "--max-mz": 2500.0,
        "--n-peaks": [10, 15, 25, 50],
        "--min-intensity": [0.001, 0.01, 0.1, 0.2, 0.5, 1.0],
        "--rescale-intensity": [true, false],
        "--normalize-intensity": true,
        "--normalize-mass": true,
        "--n-epochs": 20,
        "--batch-size": 128,
        "--learning-rate": 0.001,
        "--layer-config": [
            "[[$indim, 15, 2], [2, 15, $indim]]",
            "[[$indim, 2], [2, $indim]]",
            "[[$indim, 15, 10, 2], [2, 10, 15, $indim]]",
            "[[$indim, 15, 10, 5, 2], [2, 5, 10, 15, $indim]]",
            "[[$indim, 15, 2], [2, $indim]]",
            "[[$indim, 2], [2, 15, $indim]]",
            "[[$indim, 10, 2], [2, 10, 15, $indim]]",
            "[[$indim, 15, 10, 2], [2, 10, $indim]]"
        ]
    },
    "sessions": [
        {
            "--session": "01",
            "--use-cuda": true,
            "--gpu-device": 0,
            "--beta": [0.01]
        },
        {
            "--session": "02",
            "--use-cuda": true,
            "--gpu-device": 0,
            "--beta": [0.1]
        },
        {
            "--session": "03",
            "--use-cuda": true,
            "--gpu-device": 1,
            "--beta": [0.2]
        },
        {
            "--session": "04",
            "--use-cuda": true,
            "--gpu-device": 1,
            "--beta": [0.5]
        },
        {
            "--session": "05",
            "--use-cuda": true,
            "--gpu-device": 2,
            "--beta": [1.0]
        },
        {
            "--session": "06",
            "--use-cuda": true,
            "--gpu-device": 2,
            "--beta": [2.0]
        },
        {
            "--session": "07",
            "--use-cuda": true,
            "--gpu-device": 3,
            "--beta": [5.0]
        },
        {
            "--session": "08",
            "--use-cuda": true,
            "--gpu-device": 3,
            "--beta": [10.0]
        },
        {
            "--session": "09",
            "--use-cuda": false,
            "--beta": [100.0]
        }
    ]
}