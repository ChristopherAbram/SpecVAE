{
    "name": "reg_mona_spectrum",
    "environment": "specvae",
    "script": "script/train_regressor_single.py",
    "parameters": {
        "--model-name": "reg_spectrum",
        "--dataset": "MoNA",
        "--n-samples": -1,
        "--max-mz": 2500.0,
        "--n-peaks": [50],
        "--min-intensity": [0.001],
        "--rescale-intensity": [false],
        "--normalize-intensity": true,
        "--normalize-mass": true,
        "--n-epochs": 30,
        "--batch-size": 128,
        "--learning-rate": 0.001,
        "--input-columns": "['spectrum']"
    },
    "sessions": [
        {
            "--session": "01",
            "--use-cuda": true,
            "--gpu-device": 2,
            "--target-column": ["collision_energy", "precursor_mz"]
        },
        {
            "--session": "02",
            "--use-cuda": true,
            "--gpu-device": 2,
            "--target-column": "total_exact_mass"
        }
    ]
}