{
    "name": "betavae_reg_mona_latent",
    "environment": "specvae",
    "script": "script/train_vae_reg_single.py",
    "parameters": {
        "--model-name": "betavae_reg_latent",
        "--dataset": "MoNA",
        "--n-samples": -1,
        "--max-mz": 2500.0,
        "--n-peaks": [50],
        "--min-intensity": [0.001],
        "--rescale-intensity": [false],
        "--normalize-intensity": true,
        "--normalize-mass": true,
        "--n-epochs": 30,
        "--batch-size": 128,
        "--learning-rate": 0.001,
        "--layer-config": [
            "[[$indim, 1600, 400, 3],  [3,  400, 1600, $indim]]",
            "[[$indim, 1600, 400, 10], [10, 400, 1600, $indim]]"
        ],
        "--input-columns": "['spectrum']",
        "--target-column": ["collision_energy", "precursor_mz", "total_exact_mass"]
    },
    "sessions": [
        {
            "--session": "01",
            "--use-cuda": true,
            "--gpu-device": 0,
            "--beta": 0.01
        },
        {
            "--session": "02",
            "--use-cuda": true,
            "--gpu-device": 0,
            "--beta": 0.1
        },
        {
            "--session": "03",
            "--use-cuda": true,
            "--gpu-device": 0,
            "--beta": 0.2
        },
        {
            "--session": "04",
            "--use-cuda": true,
            "--gpu-device": 0,
            "--beta": 0.5
        },
        {
            "--session": "05",
            "--use-cuda": true,
            "--gpu-device": 0,
            "--beta": 1.0
        }
    ]
}